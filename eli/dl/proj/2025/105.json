            "de": [
              "Revision der Verordnung über die Unfallversicherung (UVV)"
            ],
            "it": [
              "Revisione dell’ordinanza sull’assicurazione contro gli infortuni (OAINF)"
            ],
            "fr": [
              "Révision de l’ordonnance sur l’assurance-accidents (OLAA)"
            ]
